<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<executable>
    <name>dia_library_generation</name>
    <context>APPLICATION</context>
    <description>
    Generates DIA assay libraries using Bibliospec, specL and prozor.
    </description>
    <program>/scratch/wolski/generateSpecLibrary/fgcz_test_yaml.py</program>
    <valid>true</valid>
    <parameter>
        <modifiable>false</modifiable>
        <description>databases default location.</description>
        <key>mascot_database_location</key>
        <label>fasta databases</label>
        <required>true</required>
        <type>string</type>
        <value>/usr/local/mascot/sequence/</value>
    </parameter>
    <parameter>
        <modifiable>true</modifiable>
        <description>min number of top transitions</description>
        <key>minN</key>
        <label>min number</label>
        <required>true</required>
        <type>int</type>
        <value>5</value>
    </parameter>
    <parameter>
        <modifiable>true</modifiable>
        <description>max number of top transitions</description>
        <key>maxN</key>
        <label>max number</label>
        <required>true</required>
        <type>integer</type>
        <value>6</value>
    </parameter>
    <parameter>
        <modifiable>true</modifiable>
        <description>measurement error in Da</description>
        <key>mzError</key>
        <label>mzErrorDa</label>
        <required>true</required>
        <type>float</type>
        <value>0.05</value>
    </parameter>
</executable>
